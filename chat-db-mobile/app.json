{
  "expo": {
    "name": "meomobile",
    "slug": "meomobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1929"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lychie5.meomobile",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.lychie5.meomobile",
      "permissions": [
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0a1929"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "scheme": "meo",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c3f4cf4b-e7e4-42e8-b461-54255937df6b"
      }
    }
  }
}
