      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.meo"
    },
    "android": {
      "package": "com.anonymous.Meo"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "meo"
  }
}
